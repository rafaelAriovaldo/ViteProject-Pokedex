// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}
generator erd {
  provider = "prisma-erd-generator"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Pokemon {
  id            String @id @default(uuid())
  name          String
  numberPokedex Int
  img           String
  team  Team[] 
}

model Team {
  id     String @id @default(uuid())
  members Int 
  pokemon Pokemon[] 
  

}
